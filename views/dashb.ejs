    <body class="nav-fixed">
        <%- include('partials/nav') -%>
            <div id="layoutSidenav_content">
                <main>
                    <header class="page-header page-header-dark bg-gradient-primary-to-secondary pb-10">
                        <div class="container-xl px-4">
                            <div class="page-header-content pt-4">
                                <div class="row align-items-center justify-content-between">
                                    <div class="col-auto mt-4">
                                        <h1 class="page-header-title">
                                            <div class="page-header-icon"><i class="fa-solid fa-house"></i></div>
                                            Home
                                        </h1>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </header>
                    <!-- Main page content-->
                    <div class="container-xl px-4 mt-n10">
                        <div class="row">
                            <div class="col-xl-4 mb-4">
                                <!-- Dashboard example card 1-->
                            </div>
                            <div class="col-xl-4 mb-4">
                                <!-- Dashboard example card 3-->
                            </div>
                        </div>
                        <div class="row">
                            <div class="col-xxl-8">
                                <!-- Tabbed dashboard card example-->
                                <div class="col-xl-6 mb-4">
                                    <!-- Pie chart with legend example-->
                                    <div class="card h-100">
                                        <div class="card-header">Calendar Events</div>
                                        <ul class="events">
                                            <% for(var i=0; i<events.length; i++) {%>
                                              <li class="event">
                                                <span><%= events[i].date%></span>
                                                <span><%= events[i].title%></span>
                                                <span><%= events[i].note%></span>
                                                <span data-eventid="<%= events[i]._id %>" class="trash">‚ùå</span>
                                              </li>
                                            <% } %>
                                            </ul>
                                    </div>
                                </div>
                                <!-- Illustration dashboard card example-->
                                <div class="card mb-4">
                                    <div class="card-body py-5">
                                        <div class="d-flex flex-column justify-content-center">
                                            <div class="text-center px-0 px-lg-5">
                                                <h5>Newsfeed</h5>
                                                <h2>Latest: <%=lastDoc.msg%> <%=lastDoc.createdAt.toLocaleString()%></h2>
                                                <ul class="messages">
                                                    <% for(var i=0; i<3; i++) {%>
                                                      <li class="message">
                                                        <span><%= messages[i].name %> _Posted: </span>
                                                        <span><%= messages[i].msg %></span>
                                                        <span><%= messages[i].createdAt.toLocaleString()%></span>
                                                      </li>
                                                    <% } %>
                                                    </ul>
                                                <a class="btn btn-primary p-3" href="/newsfeed">View more</a>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <div class="row">
                                    <div class="col-xl-6 mb-4">
                                        <!-- Dashboard activity timeline example-->
                                        <div class="card card-header-actions h-100">
                                            <div class="card-header">
                                                Recent Activity
                                            </div>
                                            <div class="card-body">
                                                <div class="timeline timeline-xs">
                                                    <!-- Timeline Item 1-->
                                                    <div class="timeline-item">
                                                        <div class="timeline-item-marker">
                                                            <div class="timeline-item-marker-text">27 min</div>
                                                            <div class="timeline-item-marker-indicator bg-green"></div>
                                                        </div>
                                                        <div class="timeline-item-content">
                                                            You added a new friend
                                                        </div>
                                                    </div>
                                                    <!-- Timeline Item 2-->
                                                    <div class="timeline-item">
                                                        <div class="timeline-item-marker">
                                                            <div class="timeline-item-marker-text">58 min</div>
                                                            <div class="timeline-item-marker-indicator bg-blue"></div>
                                                        </div>
                                                        <div class="timeline-item-content">
                                                            You created a new calendar event
                                                        </div>
                                                    </div>
                                                    <!-- Timeline Item 3-->
